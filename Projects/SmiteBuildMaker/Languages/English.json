30s cooldown.","menuitems":[{"description":"Ability Type:","value":"Buff"}],"rankitems":[{"description":"Power:","value":"10+1.5 per level "},{"description":"Lifesteal:","value":"45%"},{"description":"Ability Lifesteal:","value":"15%"},{"description":"Duration","value":"4s"}]}},"basicAttack":{"itemDescription":{"cooldown":"","cost":"","description":"","menuitems":[{"description":"Damage:","value":"39 + 2.25/Lvl (+100% of Physical Power)"},{"description":"Progression:","value":"1/1/1x damage and swing time"}],"rankitems":[]}},"godAbility1_URL":"https://webcdn.hirezstudios.com/smite/god-abilities/pyroclast.jpg","godAbility2_URL":"https://webcdn.hirezstudios.com/smite/god-abilities/eruption.jpg","godAbility3_URL":"https://webcdn.hirezstudios.com/smite/god-abilities/magma-rush.jpg","godAbility4_URL":"https://webcdn.hirezstudios.com/smite/god-abilities/volcanic-lightning.jpg","godAbility5_URL":"https://webcdn.hirezstudios.com/smite/god-abilities/everlasting-flame.jpg","godCard_URL":"https://webcdn.hirezstudios.com/smite/god-cards/pele.jpg","godIcon_URL":"https://webcdn.hirezstudios.com/smite/god-icons/pele.jpg","id":3543,"latestGod":"n","ret_msg":null},{"Ability1":"Bone Rush","Ability2":"Harvest","Ability3":"Flourish","Ability4":"Grasp of Death","Ability5":"Pomegranate Seeds","AbilityId1":19948,"AbilityId2":20205,"AbilityId3":20175,"AbilityId4":20135,"AbilityId5":20197,"Ability_1":{"Description":{"itemDescription":{"cooldown":"12/11.5/11/10.5/10s","cost":"60/65/70/75/80","description":"Persephone calls forth a Skeletal Warrior from the underworld. This warrior charges forward, damaging minions and stopping on gods. Enemy gods hit are slowed for 2.5s. Wherever it stops its skull falls, becoming a Harvest Skull.","menuitems":[{"description":"Ability Type:","value":"Line, Slow, Damage"},{"description":"Range:","value":"65"}],"rankitems":[{"description":"Damage:","value":"100/150/200/250/300 (90% of your Magical Power)"},{"description":"Slow:","value":"20/22.5/25/27.5/30%"}]